{
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "CommonJS",
    "target": "es2020",
    "outDir": "./dist",
    "sourceMap": true,
    "inlineSources": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noEmitOnError": false,
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "rootDir": "src"
  },
  "include": ["src"],
  "extends": "../tsconfig.base.json"
}
